{
	"name" : "ubl/php-iiif-prezi-reader",
	"description" : "Read IIIF Presentation API resources into PHP objects that offer some data extraction methods",
	"version" : "0.1.0-dev",
	"type" : "library",
	"keywords" : [
		"iiif",
		"presentation-api"
	],
	"require" : {
		"flow/jsonpath" : ">=0.4.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~7.5"
	},
	"autoload" : {
		"psr-4" : {
			"iiif\\" : "src/iiif",
			"" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"iiif\\" : "tests/iiif",
			"" : "src"
		}
	},
	"minimum-stability" : "stable",
	"license" : "GPL-3.0-or-later"
}